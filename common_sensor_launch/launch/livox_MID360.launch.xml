<launch>
  <!-- Params -->
  <arg name="sensor_frame" default="livox_frame"/>
  <arg name="sensor_ip" default="192.168.1.156"/>
  <arg name="vehicle_mirror_param_file"/>
  <arg name="container_name" default="livox_node_container"/>

  <include file="$(find-pkg-share common_sensor_launch)/launch/livox_node_container.launch.py">
    <arg name="frame_id" value="$(var sensor_frame)"/>
    <arg name="sensor_ip" value="$(var sensor_ip)"/>
    <arg name="vehicle_mirror_param_file" value="$(var vehicle_mirror_param_file)"/>
    <arg name="use_intra_process" value="true"/>
    <arg name="use_multithread" value="false"/>
    <arg name="container_name" value="$(var container_name)"/>
  </include>
</launch>
